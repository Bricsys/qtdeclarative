    \li \c {decodeURI(encodedURI)}
    \li \c {decodeURIComponent(encodedURIComponent)}
    \li \c {encodeURI(uri)}
    \li \c {encodeURIComponent(uriComponent)}
    \li \c {escape(string)}
    \li \c {unescape(string)}
    \endlist

