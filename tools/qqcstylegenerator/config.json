        {"atom": "default", "export": ["image", "geometry"]},
        {"atom": "background", "export": ["image", "geometry"]},
        {"atom": "contentItem", "export": ["layout"]},
        {"atom": "indicator", "export": ["image", "geometry"]},
        {"atom": "label", "export": ["text", "geometry"]}
