{
    "themes": ["Light", "Dark"],
    "image formats": ["png@2x"],
    "default export": ["image", "geometry"],
    "controls": [
    {
        "name": "Button",
        "copy": ":/Button.qml",
        "component set": "ButtonTemplate${Theme}",
        "atoms": [
            {"atom": "background", "figmaPath": "ButtonBackground, background"},
            {"atom": "contentItem", "figmaPath": "contentItem", "export": "layout"},
            {"atom": "label", "figmaPath": "contentItem, label", "export": "text"}
        ],
        "states": [
            {"state": "normal", "figmaState": "normal"},
            {"state": "checked", "figmaState": "checked"},
            {"state": "checked-hovered", "figmaState": "checkedHovered"},
            {"state": "checked-pressed", "figmaState": "checkedPressed"},
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "pressed", "figmaState": "pressed"}
        ]
    },
    {
        "name": "CheckBox",
        "copy": ":/CheckBox.qml",
        "component set": "CheckBoxTemplate${Theme}",
        "contents": ["indicator", "label"],
        "atoms": [
            {"atom": "background", "figmaPath": "CheckBoxBackground, background"},
            {"atom": "label", "figmaPath": "ContentItem, Label", "export": ["geometry", "text"]},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"},
            {"atom": "indicator", "figmaPath": "ContentItem, CheckBoxIndicatorBox"}
        ],
        "states": [
            {"state": "normal", "figmaState": "normal"},
            {"state": "checked", "figmaState": "checked"},
            {"state": "checked-disabled", "figmaState": "checkedDisabled"},
            {"state": "checked-hovered", "figmaState": "checkedHovered"},
            {"state": "checked-pressed", "figmaState": "checkedPressed"},
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "disabled-partiallyChecked", "figmaState": "partiallyCheckedDisabled"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "hovered-partiallyChecked", "figmaState": "partiallyCheckedHovered"},
            {"state": "partiallyChecked", "figmaState": "partiallyChecked"},
            {"state": "partiallyChecked-pressed", "figmaState": "partiallyCheckedPressed"},
            {"state": "pressed", "figmaState": "pressed"}
        ]
    },
    {
        "name": "ComboBox",
        "copy": ":/ComboBox.qml",
        "component set": "ComboBoxTemplate${Theme}",
        "atoms": [
            {"atom": "background", "figmaPath": "ComboBoxBackground, comboControlBackground"},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"},
            {"atom": "indicator", "figmaPath": "ComboBoxHandle"},
            {"atom": "label", "figmaPath": "ContentItem, Label", "export": "text"}
        ],
        "states": [
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "hovered-open", "figmaState": "openedHovered"},
            {"state": "normal", "figmaState": "normal"},
            {"state": "open", "figmaState": "opened"},
            {"state": "open-pressed", "figmaState": "openedPressed"},
            {"state": "pressed", "figmaState": "pressed"}
        ]
    },
    {
        "name": "ComboBoxPopup",
        "copy": ":/impl/ComboBoxPopup.qml",
        "component set": "ComboBoxTemplate${Theme}",
        "atoms": [
            {"atom": "background", "figmaPath": "ComboBoxPopup, background"},
            {"atom": "contentItem", "figmaPath": "PopupLayout, ContentItem",  "export": "layout"}
        ],
        "states": [
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "hovered-open", "figmaState": "openedHovered"},
            {"state": "normal", "figmaState": "normal"},
            {"state": "open", "figmaState": "opened"},
            {"state": "open-pressed", "figmaState": "openedPressed"},
            {"state": "pressed", "figmaState": "pressed"}
        ]
    },
    {
        "name": "Frame",
        "copy": ":/Frame.qml",
        "component set": "FrameTemplate${Theme}",
        "atoms": [
            {"atom": "background", "figmaPath": "FrameBackground, background"},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"}
        ],
        "states": [
            {"state": "normal", "figmaState": "normal"},
            {"state": "disabled", "figmaState": "disabled"}
        ]
    },
    {
        "name": "ItemDelegate",
        "copy": ":/ItemDelegate.qml",
        "component set": "ItemDelegateTemplate${Theme}",
        "atoms": [
            {"atom": "background", "figmaPath": "ItemDelegateBackground, background"},
            {"atom": "contentItem", "figmaPath": "ContentItem",  "export": "layout"},
            {"atom": "label", "figmaPath": "ContentItem, Label", "export": "text"}
        ],
        "states": [
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "highlighted", "figmaState": "highlighted"},
            {"state": "highlighted-hovered", "figmaState": "highlightedHovered"},
            {"state": "highlighted-pressed", "figmaState": "highlightedPressed"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "normal", "figmaState": "normal"},
            {"state": "pressed", "figmaState": "pressed"}
        ]
    },
    {
        "name": "RadioButton",
        "copy": ":/RadioButton.qml",
        "component set": "RadioButtonTemplate${Theme}",
        "contents": ["indicator", "label"],
        "atoms": [
            {"atom": "background", "figmaPath": "RadioButtonBackground, background"},
            {"atom": "label", "figmaPath": "ContentItem, Label", "export": ["geometry", "text"]},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"},
            {"atom": "indicator", "figmaPath": "ContentItem, RadioButtonIndicator"}
        ],
        "states": [
            {"state": "normal", "figmaState": "normal"},
            {"state": "checked", "figmaState": "checked"},
            {"state": "checked-disabled", "figmaState": "checkedDisabled"},
            {"state": "checked-hovered", "figmaState": "checkedHovered"},
            {"state": "checked-pressed", "figmaState": "checkedPressed"},
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "pressed", "figmaState": "pressed"}
        ]
    },
    {
        "name": "RangeSlider",
        "copy": ":/RangeSlider.qml",
        "component set": "RangeSliderTemplate${Theme}",
        "contents": ["groove", "track", "handle"],
        "atoms": [
            {"atom": "background", "figmaPath": "RangeSliderBackground, background"},
            {"atom": "groove", "figmaPath": "RangeSliderGroove, groveBack"},
            {"atom": "track", "figmaPath": "RangeSliderTrack, trackBack"},
            {"atom": "first-handle", "figmaPath": "RangeSliderHandleFirst, handleBack"},
            {"atom": "second-handle", "figmaPath": "RangeSliderHandleSecond, handleBack"},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"}
        ],
        "states": [
            {"state": "normal", "figmaState": "normal"},
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "focused", "figmaState": "focused"},
            {"state": "focused-hovered", "figmaState": "hoveredFocused"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "handle-pressed", "figmaState": "handlePressed"},
            {"state": "handle-pressed-focused", "figmaState": "pressedFocused"}
        ]
    },
    {
        "name": "Slider",
        "copy": ":/Slider.qml",
        "component set": "SliderTemplate${Theme}",
        "contents": ["groove", "track", "handle"],
        "atoms": [
            {"atom": "background", "figmaPath": "SliderBackground, background"},
            {"atom": "groove", "figmaPath": "SliderGroove, groveBack"},
            {"atom": "track", "figmaPath": "SliderTrack, trackBack"},
            {"atom": "handle", "figmaPath": "SliderHandle, handleBack"},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"}
        ],
        "states": [
            {"state": "normal", "figmaState": "normal"},
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "pressed", "figmaState": "pressed"}
        ]
    },
    {
        "name": "SpinBox",
        "copy": ":/SpinBox.qml",
        "component set": "SpinBoxTemplate${Theme}",
        "contents": ["indicator-down", "textInput", "indicator-up"],
        "atoms": [
            {"atom": "background", "figmaPath": "spinboxBackground, background"},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"},
            {"atom": "indicator-up", "figmaPath": "ContentItem, UpIndicator"},
            {"atom": "indicator-down", "figmaPath": "ContentItem, DownIndicator"},
            {"atom": "textInput", "figmaPath": "ContentItem, textInput", "export": ["geometry", "text"]}
        ],
        "states": [
            {"state": "atlimit", "figmaState": "upDownAtLimit"},
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "down-hovered", "figmaState": "downHovered"},
            {"state": "down-pressed", "figmaState": "downPressed"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "normal", "figmaState": "normal"},
            {"state": "up-hovered", "figmaState": "upHovered"},
            {"state": "up-pressed", "figmaState": "upPressed"}
        ]
    },
    {
        "name": "Switch",
        "copy": ":/Switch.qml",
        "component set": "SwitchTemplate${Theme}",
        "contents": ["handle-background", "label"],
        "atoms": [
            {"atom": "background", "figmaPath": "SwitchBackground, background"},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"},
            {"atom": "handle", "figmaPath": "ContentItem, SwitchHandle"},
            {"atom": "handle-background", "figmaPath": "ContentItem, HandleBackground, background"},
            {"atom": "handle-contentItem", "figmaPath": "ContentItem, HandleTemplate, ContentItem", "export": "layout"},
            {"atom": "label", "figmaPath": "ContentItem, Label", "export": ["geometry", "text"]}
        ],
        "states": [
            {"state": "normal", "figmaState": "normal"},
            {"state": "checked", "figmaState": "checked"},
            {"state": "checked-hovered", "figmaState": "checkedHovered"},
            {"state": "checked-pressed", "figmaState": "checkedPressed"},
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "pressed", "figmaState": "pressed"}
        ]
    },
    {
        "name": "TabBar",
        "copy": ":/TabBar.qml",
        "component set": "TabBarTemplate${Theme}",
        "contents": ["tabButton1", "tabButton2"],
        "atoms": [
            {"atom": "background", "figmaPath": "TabBarBackground, background"},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"},
            {"atom": "tabButton1", "figmaPath": "ContentItem, TabButton1", "export": "geometry"},
            {"atom": "tabButton2", "figmaPath": "ContentItem, TabButton2", "export": "geometry"}
        ],
        "states": [
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "disabled-footer", "figmaState": "disabledFooter"},
            {"state": "normal", "figmaState": "normal"},
            {"state": "normal-footer", "figmaState": "normalFooter"}
        ]
    },
    {
        "name": "TabButton",
        "copy": ":/TabButton.qml",
        "component set": "TabButtonTemplate${Theme}",
        "atoms": [
            {"atom": "background", "figmaPath": "TabButtonBackground, background"},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"},
            {"atom": "label", "figmaPath": "ContentItem, label", "export": "text"}
        ],
        "states": [
            {"state": "checked", "figmaState": "checked"},
            {"state": "checked-disabled", "figmaState": "checkedDisabled"},
            {"state": "checked-hovered", "figmaState": "checkedHovered"},
            {"state": "checked-pressed", "figmaState": "checkedPressed"},
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "normal", "figmaState": "normal"},
            {"state": "pressed", "figmaState": "pressed"}
        ]
    },
    {
        "name": "TextArea",
        "copy": ":/TextArea.qml",
        "component set": "TextAreaTemplate${Theme}",
        "atoms": [
            {"atom": "background", "figmaPath": "TextAreaBackground, background"},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"},
            {"atom": "label", "figmaPath": "ContentItem, Label", "export": "text"}
        ],
        "states": [
            {"state": "normal", "figmaState": "normal"},
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "focused", "figmaState": "focused"},
            {"state": "focused-hovered", "figmaState": "hoveredFocused"},
            {"state": "hovered", "figmaState": "hovered"}
        ]
    },
    {
        "name": "TextField",
        "copy": ":/TextField.qml",
        "component set": "TextFieldTemplate${Theme}",
        "atoms": [
            {"atom": "background", "figmaPath": "TextFieldBackground, background"},
            {"atom": "contentItem", "figmaPath": "contentItem", "export": "layout"},
            {"atom": "label", "figmaPath": "ContentItem, Label", "export": "text"}
        ],
        "states": [
            {"state": "normal", "figmaState": "normal"},
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "focused", "figmaState": "focused"},
            {"state": "focused-hovered", "figmaState": "hoveredFocused"},
            {"state": "hovered", "figmaState": "hovered"}
        ]
    },
    {
        "name": "ToolBar",
        "copy": ":/ToolBar.qml",
        "component set": "ToolBarTemplate${Theme}",
        "contents": ["toolButton1", "toolButton2"],
        "atoms": [
            {"atom": "background", "figmaPath": "ToolBarBackground, background"},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"},
            {"atom": "toolButton1", "figmaPath": "ContentItem, ToolButton1", "export": "geometry"},
            {"atom": "toolButton2", "figmaPath": "ContentItem, ToolButton2", "export": "geometry"}
        ],
        "states": [
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "disabled-footer", "figmaState": "disabledFooter"},
            {"state": "normal", "figmaState": "normal"},
            {"state": "normal-footer", "figmaState": "normalFooter"}
        ]
    },
    {
        "name": "ToolButton",
        "copy": ":/ToolButton.qml",
        "component set": "ToolButtonTemplate${Theme}",
        "atoms": [
            {"atom": "background", "figmaPath": "ToolButtonBackground, background"},
            {"atom": "contentItem", "figmaPath": "ContentItem", "export": "layout"},
            {"atom": "label", "figmaPath": "ContentItem, label", "export": "text"}
        ],
        "states": [
            {"state": "checked", "figmaState": "checked"},
            {"state": "checked-disabled", "figmaState": "checkedDisabled"},
            {"state": "checked-hovered", "figmaState": "checkedHovered"},
            {"state": "checked-pressed", "figmaState": "checkedPressed"},
            {"state": "disabled", "figmaState": "disabled"},
            {"state": "focused", "figmaState": "focused"},
            {"state": "focused-checked", "figmaState": "checkedFocused"},
            {"state": "focused-checked-hovered", "figmaState": "checkedHoveredFocused"},
            {"state": "focused-checked-pressed", "figmaState": "checkedPressedFocused"},
            {"state": "focused-hovered", "figmaState": "hoveredFocused"},
            {"state": "focused-pressed", "figmaState": "pressedFocused"},
            {"state": "hovered", "figmaState": "hovered"},
            {"state": "normal", "figmaState": "normal"},
            {"state": "pressed", "figmaState": "pressed"}
        ]
    }
    ]
}
